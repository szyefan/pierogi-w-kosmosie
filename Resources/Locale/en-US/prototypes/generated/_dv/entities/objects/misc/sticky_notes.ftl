    .desc = { ent-PaperStickyNote.desc }
ent-PaperStickyNoteGreen = { ent-PaperStickyNote }
    .suffix = green
    .desc = { ent-PaperStickyNote.desc }
ent-PaperStickyNoteBlue = { ent-PaperStickyNote }
    .suffix = blue
    .desc = { ent-PaperStickyNote.desc }
